{
  "name": "mine",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "npm run test && open ./coverage/lcov-report/index.html"
  },
  "author": "Stephen McGuckin",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.0.2",
    "ts-jest": "^27.0.2",
    "typescript": "^4.4.4"
  },
  "dependencies": {},
  "jest": {
    "clearMocks": true,
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).ts"
    ],
    "preset": "ts-jest",
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.ts"
    ],
    "coveragePathIgnorePatterns": []
  }
}
